  object sdsPedidos: TSQLDataSet
    CommandText = 
      'SELECT PEDIDO.PEDIDO_ID, '#13#10'       CASE PEDIDO.TIPO_OPERACAO'#13#10'   ' +
      '      WHEN '#39'E'#39' THEN '#39'Entrada'#39#13#10'         WHEN '#39'S'#39' THEN '#39'Sa'#237'da'#39#13#10' ' +
      '      END TIPO_OPERACAO,'#13#10'       PEDIDO.REFERENCIA, '#13#10'       PED' +
      'IDO.NUMERO_PEDIDO,'#13#10'       PEDIDO.DATA_EMISSAO, '#13#10'       PEDIDO.' +
      'CLIENTE_ID, '#13#10'       CLIENTE.NOME CLIENTE_NOME,'#13#10'       PEDIDO.V' +
      'ALOR_TOTAL'#13#10'FROM PEDIDOS PEDIDO'#13#10'LEFT JOIN CLIENTES CLIENTE'#13#10'  O' +
      'N CLIENTE.CLIENTE_ID = PEDIDO.CLIENTE_ID'
    MaxBlobSize = -3
    Params = <>
    SQLConnection = DMConexaoDB.SQLConnection
